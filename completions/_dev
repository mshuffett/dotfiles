#compdef dev

autoload -U is-at-least

_dev() {
    typeset -A opt_args
    typeset -a _arguments_options
    local ret=1

    if is-at-least 5.2; then
        _arguments_options=(-s -S -C)
    else
        _arguments_options=(-s -C)
    fi

    local context curcontext="$curcontext" state line
    _arguments "${_arguments_options[@]}" : \
'-n[SSH without tmux (new shell)]' \
'--no-tmux[SSH without tmux (new shell)]' \
'-h[Print help]' \
'--help[Print help]' \
'-V[Print version]' \
'--version[Print version]' \
":: :_dev_commands" \
"*::: :->dev" \
&& ret=0
    case $state in
    (dev)
        words=($line[1] "${words[@]}")
        (( CURRENT += 1 ))
        curcontext="${curcontext%:*:*}:dev-command-$line[1]:"
        case $line[1] in
            (start)
_arguments "${_arguments_options[@]}" : \
'-n[SSH without tmux (new shell)]' \
'--no-tmux[SSH without tmux (new shell)]' \
'-h[Print help]' \
'--help[Print help]' \
&& ret=0
;;
(stop)
_arguments "${_arguments_options[@]}" : \
'-n[SSH without tmux (new shell)]' \
'--no-tmux[SSH without tmux (new shell)]' \
'-h[Print help]' \
'--help[Print help]' \
&& ret=0
;;
(status)
_arguments "${_arguments_options[@]}" : \
'-n[SSH without tmux (new shell)]' \
'--no-tmux[SSH without tmux (new shell)]' \
'-h[Print help]' \
'--help[Print help]' \
&& ret=0
;;
(sync-creds)
_arguments "${_arguments_options[@]}" : \
'-n[SSH without tmux (new shell)]' \
'--no-tmux[SSH without tmux (new shell)]' \
'-h[Print help]' \
'--help[Print help]' \
&& ret=0
;;
(completions)
_arguments "${_arguments_options[@]}" : \
'-n[SSH without tmux (new shell)]' \
'--no-tmux[SSH without tmux (new shell)]' \
'-h[Print help]' \
'--help[Print help]' \
':shell -- Shell to generate completions for:(bash elvish fish powershell zsh)' \
&& ret=0
;;
(help)
_arguments "${_arguments_options[@]}" : \
":: :_dev__help_commands" \
"*::: :->help" \
&& ret=0

    case $state in
    (help)
        words=($line[1] "${words[@]}")
        (( CURRENT += 1 ))
        curcontext="${curcontext%:*:*}:dev-help-command-$line[1]:"
        case $line[1] in
            (start)
_arguments "${_arguments_options[@]}" : \
&& ret=0
;;
(stop)
_arguments "${_arguments_options[@]}" : \
&& ret=0
;;
(status)
_arguments "${_arguments_options[@]}" : \
&& ret=0
;;
(sync-creds)
_arguments "${_arguments_options[@]}" : \
&& ret=0
;;
(completions)
_arguments "${_arguments_options[@]}" : \
&& ret=0
;;
(help)
_arguments "${_arguments_options[@]}" : \
&& ret=0
;;
        esac
    ;;
esac
;;
        esac
    ;;
esac
}

(( $+functions[_dev_commands] )) ||
_dev_commands() {
    local commands; commands=(
'start:Start the server' \
'stop:Stop the server (save credits)' \
'status:Check server status' \
'sync-creds:Manually sync Claude OAuth credentials' \
'completions:Generate shell completions' \
'help:Print this message or the help of the given subcommand(s)' \
    )
    _describe -t commands 'dev commands' commands "$@"
}
(( $+functions[_dev__completions_commands] )) ||
_dev__completions_commands() {
    local commands; commands=()
    _describe -t commands 'dev completions commands' commands "$@"
}
(( $+functions[_dev__help_commands] )) ||
_dev__help_commands() {
    local commands; commands=(
'start:Start the server' \
'stop:Stop the server (save credits)' \
'status:Check server status' \
'sync-creds:Manually sync Claude OAuth credentials' \
'completions:Generate shell completions' \
'help:Print this message or the help of the given subcommand(s)' \
    )
    _describe -t commands 'dev help commands' commands "$@"
}
(( $+functions[_dev__help__completions_commands] )) ||
_dev__help__completions_commands() {
    local commands; commands=()
    _describe -t commands 'dev help completions commands' commands "$@"
}
(( $+functions[_dev__help__help_commands] )) ||
_dev__help__help_commands() {
    local commands; commands=()
    _describe -t commands 'dev help help commands' commands "$@"
}
(( $+functions[_dev__help__start_commands] )) ||
_dev__help__start_commands() {
    local commands; commands=()
    _describe -t commands 'dev help start commands' commands "$@"
}
(( $+functions[_dev__help__status_commands] )) ||
_dev__help__status_commands() {
    local commands; commands=()
    _describe -t commands 'dev help status commands' commands "$@"
}
(( $+functions[_dev__help__stop_commands] )) ||
_dev__help__stop_commands() {
    local commands; commands=()
    _describe -t commands 'dev help stop commands' commands "$@"
}
(( $+functions[_dev__help__sync-creds_commands] )) ||
_dev__help__sync-creds_commands() {
    local commands; commands=()
    _describe -t commands 'dev help sync-creds commands' commands "$@"
}
(( $+functions[_dev__start_commands] )) ||
_dev__start_commands() {
    local commands; commands=()
    _describe -t commands 'dev start commands' commands "$@"
}
(( $+functions[_dev__status_commands] )) ||
_dev__status_commands() {
    local commands; commands=()
    _describe -t commands 'dev status commands' commands "$@"
}
(( $+functions[_dev__stop_commands] )) ||
_dev__stop_commands() {
    local commands; commands=()
    _describe -t commands 'dev stop commands' commands "$@"
}
(( $+functions[_dev__sync-creds_commands] )) ||
_dev__sync-creds_commands() {
    local commands; commands=()
    _describe -t commands 'dev sync-creds commands' commands "$@"
}

if [ "$funcstack[1]" = "_dev" ]; then
    _dev "$@"
else
    compdef _dev dev
fi
